body {font-family: lato;}

#page_wrapper { width: 100%; max-width: 2000px; overflow: hidden;  position: relative;  margin: 0 auto;}

h1,h2,h3,h4 {font-size: inherit; color: inherit;  font-weight:inherit; line-height: inherit; margin-top: 0; margin-bottom: 0; display: inherit;}

.transition_0_5 {-webkit-transition: all 0.5s ease-in-out; -moz-transition: all 0.5s ease-in-out; -o-transition: all 0.5s ease-in-out; transition: all 0.5s ease-in-out;}
.transition_0_2 {-webkit-transition: all 0.2s ease-in-out; -moz-transition: all 0.2s ease-in-out; -o-transition: all 0.2s ease-in-out; transition: all 0.2s ease-in-out;}

.mTop20{ margin-top: 20px;}
.mTop5{ margin-top: 5px;}
.clear {clear: both;}
.no-padding {padding: 0;}
.no-margin {margin: 0;}
.MTB5 {margin-top: 10px; margin-bottom: 10px;}
.MBT5 {margin-top: 10px; margin-bottom: 10px;}
.alignC {text-align: center;}
.podkladka20 {height: 20px;}
.title-main {background: url(images/box-title-bg.png) repeat-x center left; padding-left: 0; margin-top: 10px; margin-bottom: 10px;}
.title-main .title-main-span{font-weight: 700; padding-right: 25px; background: white; color: #04645e; font-size:16px;}
.table-inner {display: table-cell; vertical-align: middle;} 
/*Header*/
header #top {background: #282c2c; min-height: 60px; }
.menu-top {color: white; margin-bottom: 0px; padding-left: 0px;}
.menu-top li {display: inline-block;  text-transform: uppercase; padding: 20px 10px; border-bottom: #282c2c 6px solid;}
.menu-top li a{color: #b1b3b3; }
.menu-top li a:hover{color: white; text-decoration:none; }
.menu-top li.active a{color: white;}
.menu-top li.active{color: white; text-decoration:none;   border-bottom: solid 6px #146665;}
.menu-top .menu-top_lv2 { display: none; position: absolute; padding-left: 0; z-index: 10;}
.menu-top .menu-top_lv2 li{ display: block; background: #282C2C; padding: 10px; color: #04645e;}
.menu-top .menu-top_lv2 li a{color: #b1b3b3;}
.menu-top .menu-top_lv2 li a.active{color: white;}
.menu-top li.active .menu-top_lv2 a {}
.glyphicon-menu-down{font-size: 10px;}
.menu-top-image {position: absolute; top: 22px;}
@media (min-width: 996px) {
	.menu-top li:hover .menu-top_lv2 {display: block;}

}
@media (max-width: 996px) {
	.menu-top-div {padding-top: 50px;}
	.menu-top li.active {border:none;}
	.menu-top li {display: block; border:none; padding: 8px 10px; }
	.menu-top li .menu-top_lv2 {position: relative;}
}
.lower-right  div {float:right;}
.menu-bottom {color: white; margin-bottom: 0px; padding-left: 0px;  width: auto;}
.menu-bottom li {display: inline-block;  text-transform: uppercase; padding: 20px 10px; }
.menu-bottom li a{color: black; }
.menu-bottom li a:hover{color: #b1b3b3; text-decoration:none; }
.menu-bottom li.active a{color: #b1b3b3;}
.menu-bottom li.active{color: #b1b3b3; text-decoration:none; }
#go-top {float: right; margin-top: 10px; padding: 10px 10px; background: #088380; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px;}
#go-top  {color: white;}
.go-top  {float: right;}
#go-top:hover  {color: white; text-decoration: none; background: #199491;}
#go-top:active  {color: white; text-decoration: none; background: #199491;}
.menu-bottom {float: right; }
.lower2-right {color: white; font-size:10px;}

/*Rejestracja*/
header .rejestracja {position:absolute; top: 21px; right: 10px;}
header .rejestracja .rejestracja_1 {padding: 10px 20px; margin: 5px; color: white;  background: #04645e; border: 1px solid #04645e;}
header .rejestracja .rejestracja_1:hover{text-decoration:none; color: #9e9f9f; border: 1px solid #04645e; background:none; }
header .rejestracja .rejestracja_2 {padding: 10px 20px; margin: 5px; color: #9e9f9f; border: 1px solid #04645e;}
header .rejestracja .rejestracja_2:hover {text-decoration:none; color: white;  background: #04645e; border: 1px solid #04645e;}
header .rejestracja .rejestracja_2 img {margin-left: 2px; margin-right:2px;}

header #lower {background: white; min-height: 70px; padding-top: 20px; padding-bottom: 20px;}
.social {padding-top: 5px; }
.social .social-text {display: inline-block; font-size: 13px;  color: #abc0c2;}
.social .social-text span {margin-top:2px;}
.social .social-list{display: inline-block;}
.social .social-list li{display: inline-block; margin-left: 5px; margin-right: 5px;}
.social .social-list li a{}
.social .social-list li a img{ opacity: 0.8;}
.social .social-list li a img:hover{ opacity: 1;}
.social .social2 {opacity: 0.5 !important;}
.social .social2:hover{opacity: 0.8 !important;}
.wyszukiwarka { float: right;}
.wyszukiwarka  form{ position : relative;}
.wyszukiwarka  form input.szukaj{ padding: 5px 30px 5px 5px; outline: none; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; border: 1px solid #dedede; }
.wyszukiwarka  form .submit{ position: absolute; top:5px; right: 13px;}


@media (max-width: 996px) {
	header .rejestracja .rejestracja_1 {padding: 10px 10px;}
	header .rejestracja .rejestracja_2 {padding: 10px 10px;}
}

footer {}
footer #top2 {background: #f3f3f3; min-height: 50px; padding-top: 20px;}

footer #lower2 {background: #282c2c; min-height: 50px; color: white; padding-top: 20px;padding-bottom: 20px; font-size: 10px;}
footer #lower2  .lower2-right{ text-align:right;}
footer #lower2  .lower2-right-inner{ display: inline-block;}
footer #lower2  .lower2-right-inner a{color: white;}
footer #lower2  .lower2-right-inner a:hover{color: white;}
footer #lower2  .icons{ display: inline-block;}
footer #lower2  .icons img{ margin-top: -12px;}

#kontakt {}
#kontakt .mainmap-outter{width:100%; height: 265px; position:relative; margin-bottom: 20px;}
#kontakt .mainmap-outter #mainmap{ width:100%; height: 100%;}
#kontakt .mainmap-outter .mainmap-more{ position: absolute; bottom: 14px; right: 0px; background: rgba(255,255,255,0.8); padding: 5px 30px;}
#kontakt .mainmap-outter .mainmap-more a{ text-decoration:none; color: #589e9c;}
#kontakt .mainmap-outter .mainmap-more a:hover{ text-decoration:none; color: #589e9c;}
#kontakt .kontakt-text{ font-size: 15px;  margin-bottom: 15px;}
#kontakt .kontakt-text .kontakt-title {margin-bottom: 45px; position: relative;}
#kontakt .kontakt-text .kontakt-title:after { display: block; content: " "; position: absolute; width: 65px; height: 1px; background: #8a8a8a;  bottom: -20px;}
#kontakt .kontakt-text .kontakt-title h2{ color: #262626; font-size:22px;}
#kontakt .kontakt-text .kontakt-title h1{ color: #088380; font-size:22px; font-weight: 700;}
#kontakt .kontakt-formularz {}
#kontakt .kontakt-formularz  #formularz{ padding: 20px 15px; background: #f6f6f6; color: #7c7c7c;}
#kontakt .kontakt-formularz  #formularz .poletekstowe {margin-top: 6px; margin-bottom: 6px; width: 100%; font-size: 15px; padding-left: 20px; padding-right: 20px; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; outline: none; border: none;}
#kontakt .kontakt-formularz  #formularz input{ height: 45px; }
#kontakt .kontakt-formularz  #formularz textarea{ height: 155px; padding-top: 10px; padding-bottom: 10px; resize: none;}
#kontakt .kontakt-formularz  #formularz .error{ -webkit-box-shadow: inset 0px 0px 4px 0px rgba(252,0,0,1); -moz-box-shadow: inset 0px 0px 4px 0px rgba(252,0,0,1); box-shadow: inset 0px 0px 4px 0px rgba(252,0,0,1); border: 1px solid #e90000;}
#kontakt .kontakt-formularz  #formularz .error{ background: url(images/x.png) center right no-repeat; padding-right: 50px;}
#kontakt .kontakt-formularz  #formularz .correct{ -webkit-box-shadow: inset 0px 0px 4px 0px rgba(8,131,128,1); -moz-box-shadow: inset 0px 0px 4px 0px rgba(8,131,128,1); box-shadow: inset 0px 0px 4px 0px rgba(8,131,128,1); border: 1px solid #088380;}
#kontakt .kontakt-formularz  #formularz .correct{ background: url(images/ok.png) center right no-repeat; padding-right: 50px;}
#kontakt .kontakt-formularz  #formularz .info { font-size: 11px;}
#kontakt .kontakt-formularz  #formularz .info span{ color: red;}
#kontakt .kontakt-formularz  #formularz .submit-div{margin-top: 30px; margin-bottom: 30px;}
#kontakt .kontakt-formularz  #formularz .submit-div a.submit{background: #088380; color: white; padding: 20px 50px; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; text-transform: uppercase;}
#kontakt .kontakt-formularz  #formularz .submit-div a.submit img{ margin-left: 10px; margin-top: -3px;}
#kontakt .kontakt-formularz  #formularz .submit-div a.submit:hover{background: #199491; text-decoration: none;}
#kontakt .kontakt-formularz  #formularz .captcha{}
@media (max-width: 1800px) {
	#kontakt .kontakt-formularz  #formularz .submit-div a.submit{ padding: 20px 40px; }
}
@media (max-width: 996px) {

	#kontakt .kontakt-formularz  #formularz .captcha{text-align:center;}
	#kontakt .kontakt-formularz  #formularz .captcha div{display: inline-block;}
	#kontakt .kontakt-formularz  #formularz .submit-div {text-align:center; }
	#kontakt .kontakt-formularz  #formularz .submit-div  .submit{}
}
/*Aktualnosci*/
