.aktualnosci .lista-aktualnosci .single-aktualnosci-first .single-aktualnosci-first-text .single-aktualnosci-first-text-inner .single-aktualnosci-first-text-inner-1{ height: 60%; display: table;}
.aktualnosci .lista-aktualnosci .single-aktualnosci-first .single-aktualnosci-first-text .single-aktualnosci-first-text-inner .single-aktualnosci-first-text-inner-2{ height: 40%; display: table;}
