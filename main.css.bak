.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li{display: inline-block; text-align: center;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li a{ font-weight: 700; color: #088380; margin-left: 2px; margin-right: 2px; height: 40px; display: inline-block; line-height: 40px; min-width: 40px; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; background: #f6f6f6;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li a.skrajne{ color: #f6f6f6; padding-left: 30px; padding-right: 30px; background: #088380;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li.kreska{ margin-left: 20px; margin-right: 20px; color: white; height: 40px; line-height: 40px; width:1px; padding-left: 0px; padding-right: 0px; background: #088380;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li.kropki{width: 40px;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li a:hover{ text-decoration: none; background: #088380; color:#f6f6f6;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li a.active{ text-decoration: none; background: #088380; color:#f6f6f6;}
.aktualnosci .paginacja-aktualnosci ul.paginacja-aktualnosci-list li a.skrajne:hover{ color: #088380; padding-left: 30px; padding-right: 30px; background: #f6f6f6;}
.aktualnosci .article-1{font-size: 20px; color: white; margin-bottom: 10px; display: block;}
.aktualnosci .article-2{color: white; font-size: 25px; display: block;}
.aktualnosci .article-3{color: white; font-size: 11px;}
.aktualnosci .aktualnosci-wiecej {text-align: center; min-height: 70px;}
.aktualnosci .aktualnosci-wiecej .aktualnosci-wiecej-inner { padding-top: 20px; padding-bottom: 20px;}
.aktualnosci .aktualnosci-wiecej .aktualnosci-wiecej-inner a { padding: 10px 20px; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; background: #088380; color: white;}
.aktualnosci .aktualnosci-wiecej .aktualnosci-wiecej-inner a:hover {background: #199491; color: white; text-decoration: none;}
.aktualnosci .aktualnosci-pojedynczy { padding-top: 50px;padding-bottom: 50px;}
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-inner { }
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-autor {margin-top: 30px; background: #e0e0e0; min-height: 50px; padding-top: 15px; padding-bottom: 20px; }
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-autor .autor{ color: #076a67;  margin-bottom:10px;}
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-autor .danezdjecie{ color: #076a67; }
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-autor .danezdjecie .zdjecie{ height: 83px; width : 83px; float: left; margin-right: 20px;}
.aktualnosci .aktualnosci-pojedynczy .aktualnosci-pojedynczy-autor .danezdjecie .dane{ color: #076a67; }

